<?xml version="1.0" encoding="utf-8"?>
<component version="1.0" xmlns="http://schemas.circuit-diagram.org/circuitDiagramDocument/2012/component/xml">
  <declaration>
    <meta name="name" value="Label" />
    <meta name="minsize" value="50" />
    <meta name="canresize" value="false" />
    <meta name="author" value="Circuit Diagram" />
    <meta name="additionalinformation" value="http://www.circuit-diagram.org/" />
    <meta name="guid" value="d93dd6a8-4f47-441a-83d9-e3aa719a2ecb" />
    <meta name="implementset" value="http://schemas.circuit-diagram.org/circuitDiagramDocument/2012/components/misc" />
    <meta name="implementitem" value="label" />
    
    <property name="Text" type="string" default="(empty)" serialize="text" display="Text" />
  </declaration>
  <render>
    <group conditions="!$Text(empty)">
      <text value="$Text" x="_Start" y="_Start" align="CentreCentre" />
    </group>
    <group conditions="$Text(empty)">
      <text value="(empty)" x="_Start" y="_Start" align="CentreCentre" />
    </group>
  </render>
</component>