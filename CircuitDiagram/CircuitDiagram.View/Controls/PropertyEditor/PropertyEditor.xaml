<UserControl x:Class="CircuitDiagram.View.Controls.PropertyEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CircuitDiagram.View.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <ItemsControl ItemsSource="{Binding EditableComponents, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type local:PropertyEditor}}}">
            
        </ItemsControl>
    </Grid>
</UserControl>
